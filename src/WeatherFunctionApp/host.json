{
  "version": "2.0",
  "extensions": {
    "http": {
      "maxConcurrentRequests": 100000,
      "maxOutstandingRequests": 50000
    },
    "queues": {
      "maxPollingInterval": 2000,
      "batchSize": 32,
      "maxDequeueCount": 5,
      "newBatchThreshold": 16
    }
  },
  "durableTask": {
    "partitionCount": 16
  }
}